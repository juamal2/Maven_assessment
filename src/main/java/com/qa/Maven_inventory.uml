              <language>java</language>
              <body>String result = &quot;failed&quot;;&#xD;
		if (Boolean.TRUE.equals(isValid())) {&#xD;
			try {&#xD;
				this.stmt = this.connection.createStatement();&#xD;
					this.resultSet = stmt.executeQuery(query);&#xD;
