            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_EMJkJ08EEeqZV74XuWPFSg" name="getItemFromId" specification="_ELNwAE8EEeqZV74XuWPFSg">
              <ownedParameter xmi:id="_EMJkKE8EEeqZV74XuWPFSg" name="t" type="_EGOiUE8EEeqZV74XuWPFSg"/>
              <ownedParameter xmi:id="_EMJkKU8EEeqZV74XuWPFSg" name="database" type="_EFW_oE8EEeqZV74XuWPFSg"/>
              <ownedParameter xmi:id="_EMJkKk8EEeqZV74XuWPFSg" type="_EGOiUE8EEeqZV74XuWPFSg" direction="return"/>
              <language>java</language>
              <body>if (t.getId() >= 0) {&#xD;
			try {&#xD;
			Item item = new Item(t.getId());&#xD;
			String valueStr = database.selectQuery(&quot;SELECT value FROM items WHERE id = '&quot; + t.getId() + &quot;'&quot;);&#xD;
			valueStr = valueStr.replace(&quot;value = &quot;, &quot;&quot;).trim();&#xD;
			item.setValue(Double.parseDouble(valueStr));&#xD;
			String nameStr = database.selectQuery(&quot;SELECT name FROM items WHERE id = '&quot; + t.getId() + &quot;'&quot;);&#xD;
			item.setName(nameStr.replace(&quot;name = &quot;, &quot;&quot;).trim());&#xD;
			LOGGER.info(&quot;you selected: &quot; + item.getName() + &quot;cost: &quot; + item.getValue() );&#xD;
			return item;&#xD;
			}&#xD;
			catch(NumberFormatException e) {&#xD;
				LOGGER.info(&quot;invalid id&quot;);&#xD;
				return new Item(-1);&#xD;
			}&#xD;
		}else {	return new Item(-1);}&#xD;
&#xD;
	</body>
