            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_EMHH408EEeqZV74XuWPFSg" name="query" specification="_EFdtUE8EEeqZV74XuWPFSg">
              <ownedParameter xmi:id="_EMHH5E8EEeqZV74XuWPFSg" name="query">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <ownedParameter xmi:id="_EMHH5U8EEeqZV74XuWPFSg" direction="return">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <language>java</language>
              <body>int result =-1;&#xD;
		if (Boolean.TRUE.equals(isValid())) {&#xD;
			try {&#xD;
				this.stmt = this.connection.createStatement();&#xD;
				stmt.executeUpdate(query, Statement.RETURN_GENERATED_KEYS);&#xD;
				String keys = resultSetToString(stmt.getGeneratedKeys());&#xD;
				return Integer.parseInt(keys.replace(&quot;GENERATED_KEY = &quot;, &quot;&quot;).trim());&#xD;
			}catch (SQLException ex) {&#xD;
				LOGGER.info(ex.getMessage());&#xD;
			}catch (NumberFormatException ex) {&#xD;
				LOGGER.info(&quot;No primary keys returned&quot;);&#xD;
			}&#xD;
			finally {&#xD;
				this.clear();&#xD;
			}&#xD;
		}&#xD;
		return result;&#xD;
	</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_EMHH5k8EEeqZV74XuWPFSg" name="resultSetToString" specification="_ELmxkE8EEeqZV74XuWPFSg">
              <ownedParameter xmi:id="_EMHu8E8EEeqZV74XuWPFSg" name="resultset" type="_EK4Y0E8EEeqZV74XuWPFSg"/>
              <ownedParameter xmi:id="_EMHu8U8EEeqZV74XuWPFSg" direction="return">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <language>java</language>
              <body>String result = &quot;&quot;;&#xD;
		try {&#xD;
			ResultSetMetaData meta = resultset.getMetaData();&#xD;
			while(resultset.next()) {&#xD;
				String row = &quot;&quot;;&#xD;
				for (int i = 1; i &lt;=  meta.getColumnCount(); i++) {&#xD;
					row +=  meta.getColumnLabel(i) + &quot; = &quot; + resultset.getString(i) + &quot;	 &quot;;&#xD;
				}&#xD;
				result += &quot;\n&quot; + row ;&#xD;
			}&#xD;
		}&#xD;
		catch(SQLException e) {&#xD;
			result = &quot;null&quot;;&#xD;
		}&#xD;
&#xD;
		return result;&#xD;
	</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_EMHu8k8EEeqZV74XuWPFSg" name="clear" specification="_EFeUYE8EEeqZV74XuWPFSg">
              <language>java</language>
              <body>if (this.resultSet != null) {&#xD;
			try {this.resultSet.close();&#xD;
			}catch(SQLException sqlEx) {LOGGER.info(&quot;failed to close resultset&quot;);}&#xD;
			this.resultSet = null;&#xD;
		}&#xD;
		if (this.stmt != null) {&#xD;
			try {this.stmt.close();&#xD;
			}catch(SQLException sqlEx) {LOGGER.info(&quot;failed to close statement&quot;);}&#xD;
			this.stmt = null;&#xD;
		}&#xD;
	</body>
            </ownedBehavior>
